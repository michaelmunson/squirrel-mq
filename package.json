{
  "name": "squirrelify",
  "version": "0.5.0",
  "description": "PostgreSQL framework for bootstrapping your database, api, and client",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./schema": "./dist/schema/index.js",
    "./schema/fields": "./dist/schema/fields/fields.js",
    "./client": "./dist/client/index.js",
    "./api": "./dist/api/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "create-docs": "typedoc --out ./docs ./src/index.ts ./src/api/index.ts ./src/client/index.ts ./src/schema/index.ts",
    "build": "tsc",
    "test": "jest",
    "cli-init": "npm i && npm run build && chmod +x ./dist/cli/index.js && npm link --local"
  },
  "author": "Michael Munson",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelmunson/squirrel-mq"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "@types/pg": "^8.11.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.6",
    "typescript": "^5.6.3"
  }
}